#!/usr/bin/env ruby
# encoding: utf-8

$LOAD_PATH.unshift(File.dirname(File.realpath(__FILE__)) + '/../lib')
require 'papers'

generator = Papers::ManifestGenerator.new

begin
  generator.generate!
rescue Papers::FileExistsError => e
  warn "Error: 'papers_manifest.yml' already exists at '#{e.message}'. Aborting..."
end